        "alterfw/php-form-generator": "dev-master",
        "alterfw/wordpress-for-developers": "dev-master",
        "valendesigns/option-tree": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "files": ["core/main.php"]
    }
